<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FindTutor1">


    <TextView
        android:id="@+id/find_tutor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find a Tutor"
        android:textSize="30sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="52dp" />


    <TextView
        android:id="@+id/subject_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What subject do you need help in?"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="132dp" />

    <ToggleButton
        android:id="@+id/find_tutor_compsci"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="Comp Sci"
        android:textOn="Comp Sci"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="316dp"
        tools:layout_editor_absoluteY="341dp" />

    <ToggleButton
        android:id="@+id/find_tutor_math"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="Math"
        android:textOn="Math"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="252dp" />

    <ToggleButton
        android:id="@+id/find_tutor_english"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="English"
        android:textOn="English"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="252dp" />

    <ToggleButton
        android:id="@+id/find_tutor_socsci"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="Social Science"
        android:textOn="Comp Sci"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="252dp" />

    <ToggleButton
        android:id="@+id/find_tutor_science"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="Science"
        android:textOn="Science"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="341dp" />

    <ToggleButton
        android:id="@+id/find_tutor_lang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="Foreign Language"
        android:textOn="Foreign Language"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="341dp" />

    <Button
        android:id="@+id/find_tutor_submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="514dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>