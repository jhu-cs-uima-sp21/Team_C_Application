<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/hello_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello, name!"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Students Contacted"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="98dp" />

    <RelativeLayout
        android:layout_width="416dp"
        android:layout_height="344dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.358">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="49dp"
            android:layout_marginTop="41dp"
            android:background="@drawable/ic_notifications_black_24dp"
            android:text="Hello" />

    </RelativeLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="582dp" />

    <TextView
        android:id="@+id/your_subjects"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Your Subjects"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="533dp" />

    <TextView
        android:id="@+id/num_students_contacted"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:text="0"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="666dp" />

    <TextView
        android:id="@+id/num_questions_answered"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_marginStart="272dp"
        android:text="0"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="666dp" />

    <TextView
        android:id="@+id/your_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your History"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="628dp" />

    <TextView
        android:id="@+id/questions_answered"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="252dp"
        android:text="Questions Answered"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="719dp" />

    <TextView
        android:id="@+id/students_contacted2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Students Contacted"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="718dp" />


</androidx.constraintlayout.widget.ConstraintLayout>